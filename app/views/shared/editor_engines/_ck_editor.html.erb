<%= javascript_include_tag Ckeditor.cdn_url %>
<%= javascript_include_tag 'ckeditor/config.js' %>

<script>
    $(function () {
        <% ids.each do |id| %>
        if ($("#<%= id %>").length > 0) {
            var config = {}
            CKEDITOR.editorConfig(config)
            config.removeDialogTabs = '';
            CKEDITOR.replace("<%= id %>", config );
        }
        <% end %>
    });
</script>
